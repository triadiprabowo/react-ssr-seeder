{
  "name": "react-ssr-seeder",
  "author": "",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build:prod": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.prod.js",
    "watch": "./node_modules/.bin/webpack -w",
    "start:server": "./node_modules/.bin/nodemon server.js",
    "start:prod": "NODE_ENV=production pm2 start server.js --name=React SSR",
    "stop:prod": "pm2 stop server.js",
    "restart:prod": "pm2 restart server.js",
    "start": "NODE_ENV=development npm run build",
    "poststart": "NODE_ENV=development npm-run-all --parallel watch start:server",
    "test": "NODE_ENV=test jest"
  },
  "license": "MIT",
  "description": "",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.7.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.12.5",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "pm2": "^2.10.3",
    "postcss-loader": "^2.1.5",
    "postcss-url": "^7.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "isomorphic-fetch": "^2.2.1",
    "q": "^1.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
